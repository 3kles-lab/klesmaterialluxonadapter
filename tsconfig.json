{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "kles-material-luxon-adapter": [
        "dist/kles-material-luxon-adapter/kles-material-luxon-adapter",
        "dist/kles-material-luxon-adapter"
      ],
      "kles-material-luxon-adapter/*": [
        "dist/kles-material-luxon-adapter/kles-material-luxon-adapter",
        "dist/kles-material-luxon-adapter"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}